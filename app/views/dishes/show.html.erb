<h3>Dish Info</h3>

<div id="dish-info">
  <p>Name -<%= @dish.name %></p>
  <p>Description- <%= @dish.description %></p>

</div>

<div id="dish-ingredients">
  <% @dish.ingredients.each do |ingredient| %>
    <p>Name -<%= ingredient.name %></p>
    <p>Calories- <%= ingredient.calories %></p>
  <% end %>
</div>
